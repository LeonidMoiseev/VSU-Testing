<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lion.test_rating">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AccountStudentActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AccountTeacherActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RegistrationStudentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegistrationTeacherActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".StartTestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultTestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PreRegistrationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultsStudentsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ResultsTeachersActivity"
            android:screenOrientation="portrait" ></activity>
    </application>

</manifest>